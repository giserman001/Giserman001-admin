{
  "name": "ant-design-pro-vue3",
  "type": "module",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "dev": "vite",
    "dev:debug": "vite --debug",
    "build": "cross-env vite build --mode production",
    "build:test": "cross-env vite build --mode test",
    "preview": "vite preview",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "lint": "eslint --cache --max-warnings 0  \"src/**/*.{js,jsx,ts,tsx,vue,json,less}\" --fix",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm bootstrap",
    "report": "cross-env npm_lifecycle_event=report vite build",
    "pushall": "git push && git push origin-github",
    "lint:lint-staged": "lint-staged",
    "prepare": "husky",
    "commit": "git add . && git-cz"
  },
  "dependencies": {
    "@aesoper/normal-utils": "^0.1.5",
    "@ant-design/icons-vue": "^7.0.1",
    "@antv/g2plot": "^2.4.33",
    "@logicflow/core": "2.0.13",
    "@logicflow/extension": "2.0.17",
    "@vue/server-renderer": "^3.5.13",
    "@vueuse/core": "^13.1.0",
    "ant-design-vue": "^4.2.6",
    "axios": "^1.8.4",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "dotenv": "16.4.5",
    "driver.js": "^1.3.5",
    "echarts": "^5.6.0",
    "echarts-liquidfill": "^3.1.0",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "nanoid": "^5.1.5",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "qs": "^6.14.0",
    "tinycolor2": "^1.6.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.13",
    "vue-clipboard3": "^2.0.0",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@faker-js/faker": "^9.7.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.1",
    "@unocss/eslint-plugin": "^66.1.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.1",
    "eslint": "^9.25.1",
    "eslint-plugin-format": "^1.0.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "lint-staged": "^16.0.0",
    "minimist": "^1.2.8",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^5.14.0",
    "sharp": "^0.34.1",
    "shelljs": "^0.9.2",
    "typescript": "~5.8.3",
    "unocss": "66.1.0-beta.12",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite": "^6.3.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-fake-server": "^2.2.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "^2.2.10"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue,json,less}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "volta": {
    "node": "22.14.0",
    "pnpm": "9.3.0"
  }
}
